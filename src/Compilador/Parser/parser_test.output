Grammar

    0 $accept: programa $end

    1 programa: ID '{' lista_sentencias '}'
    2         | error '{' lista_sentencias '}'
    3         | '{' lista_sentencias '}'
    4         | ID lista_sentencias '}'
    5         | ID '{' lista_sentencias

    6 sentencia: sentencia_declarativa
    7          | sentencia_ejecutable

    8 sentencia_declarativa: declaracion_variable ';'
    9                      | declaracion_funcion

   10 sentencia_ejecutable: asignacion ';'
   11                     | asignacion
   12                     | control
   13                     | llamada_funcion ';'
   14                     | llamada_funcion
   15                     | print ';'
   16                     | print
   17                     | retorno ';'
   18                     | retorno

   19 declaracion_variable: tipo lista_identificadores
   20                     | error lista_identificadores
   21                     | tipo asignacion
   22                     | error asignacion

   23 declaracion_funcion: tipo ID '(' lista_parametros_formales ')' '{' lista_sentencias '}'
   24                    | tipo ID '(' ')' '{' lista_sentencias '}'
   25                    | error ID '(' lista_parametros_formales ')' '{' lista_sentencias '}'
   26                    | tipo ID '(' lista_parametros_formales ')' '{' '}'
   27                    | tipo ID '(' ')' '{' '}'

   28 lista_sentencias: sentencia
   29                 | lista_sentencias sentencia

   30 lista_sentencias_ejecutables: sentencia_ejecutable
   31                             | lista_sentencias_ejecutables sentencia_ejecutable

   32 lista_parametros_formales: parametro_formal
   33                          | lista_parametros_formales ',' parametro_formal
   34                          | lista_parametros_formales error parametro_formal

   35 lista_parametros_reales: parametro_real_compuesto
   36                        | lista_parametros_reales ',' parametro_real_compuesto
   37                        | lista_parametros_reales error parametro_real_compuesto

   38 lista_identificadores: IDCOMP
   39                      | lista_identificadores ',' IDCOMP
   40                      | lista_identificadores error IDCOMP
   41                      | ID
   42                      | lista_identificadores ',' ID

   43 parametro_formal: CR tipo ID
   44                 | tipo ID
   45                 | LAMBDA tipo ID
   46                 | CR LAMBDA tipo ID

   47 parametro_real_compuesto: parametro_real FLECHA parametro_formal

   48 parametro_real: expresion
   49               | lambda

   50 control: sentencia_IF ';'
   51        | do_until ';'

   52 sentencia_IF: IF '(' condicion ')' '{' lista_sentencias_ejecutables '}' ENDIF
   53             | IF '(' condicion ')' '{' lista_sentencias_ejecutables '}' ELSE '{' lista_sentencias_ejecutables '}' ENDIF

   54 condicion: expresion comparador expresion

   55 do_until: DO '{' lista_sentencias_ejecutables '}' UNTIL '(' condicion ')'

   56 comparador: MENORIGUAL
   57           | MAYORIGUAL
   58           | IGUALIGUAL
   59           | DISTINTO
   60           | '>'
   61           | '<'

   62 tipo: LONG
   63     | STRING

   64 asignacion: IDCOMP ASIGNAR expresion
   65           | ID ASIGNAR expresion

   66 expresion: expresion '+' termino
   67          | expresion '-' termino
   68          | termino
   69          | TRUNC '(' expresion ')'

   70 termino: termino '*' factor
   71        | termino '/' factor
   72        | factor

   73 factor: IDCOMP
   74       | CTEL
   75       | CTEF
   76       | llamada_funcion
   77       | ID

   78 llamada_funcion: IDCOMP '(' lista_parametros_reales ')'
   79                | ID '(' lista_parametros_reales ')'
   80                | IDCOMP '(' ')'
   81                | ID '(' ')'

   82 print: PRINT '(' STRING ')'
   83      | PRINT '(' expresion ')'

   84 lambda: '(' tipo ')' '{' lista_sentencias_ejecutables

   85 retorno: RETURN '(' expresion ')'


